
<otsikko2>Kirjaudu sisään</otsikko2>
<form ng-submit="submit2()">
<div class="lead">
  Syötä käyttäjätunnus ja salasana:</br>
  </div>
  <table border="0">
      <tr>
          <td>
              <b>Käyttäjätunnus:</b>
          </td>
          <td>
              <input type="text" ng-model="username" name="text" /><br />
          </td>
      </tr>
      <tr>
        <td>
            <b>Salasana:</b>
        </td>
        <td>
            <input type="password" ng-model="password" name="text" /><br />
        </td>
      </tr>
  </table>
<input type="submit" class="btn btn-primary" id="submit" value="Kirjaudu" />
<div class="alert alert-danger" ng-if="errorMsg" role="alert">Väärä käyttäjätunnus tai salasana.</div>
