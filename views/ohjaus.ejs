<!--     OTSIKKO       -->
<!--                   -->
{{date}}
<otsikko2 ng-if="TILA === 'UUSI'||TILA === 'OIKEIN'||TILA === 'VAARIN'">
    {{asetukset.peliTyyppi}} - {{asetukset.otsikkoTaso}}   {{ pisteet.monesko }} / {{pisteet.maksimi}} - {{kayttajaNimi}}
</otsikko2>
